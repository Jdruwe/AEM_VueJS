<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:tags="[]"
        cq:template="/conf/spavue/settings/wcm/templates/single-page-template"
        cq:xfMasterVariation="{Boolean}true"
        jcr:description="Header component to be shared across multiple templates"
        jcr:primaryType="cq:PageContent"
        jcr:title="Header"
        sling:resourceType="spavue/components/page/basepage">
        <centerParsys
            jcr:primaryType="nt:unstructured"
            sling:resourceType="wcm/foundation/components/responsivegrid">
            <header
                jcr:primaryType="nt:unstructured"
                sling:resourceType="spavue/components/structure/header">
            </header>
        </centerParsys>
    </jcr:content>
</jcr:root>
